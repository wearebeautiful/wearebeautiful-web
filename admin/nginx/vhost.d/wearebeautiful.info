client_max_body_size 500M;
sendfile               on;
sendfile_max_chunk     1m;

gzip on;
gzip_disable "msie6";
gzip_vary on;
gzip_proxied any;
gzip_comp_level 6;
gzip_buffers 16 8k;
gzip_http_version 1.1;
gzip_min_length 256;
gzip_types text/plain text/css application/json application/x-javascript text/javascript image/svg+xml image/x-icon model/stl application/sla;
